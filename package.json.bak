{
  "name": "archer-avenue",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "browser": {
    "vue": "vue/dist/vue.common"
  },
  "scripts": {
    "start": "yarn files && watchify -p browserify-hmr -t [ babelify ] -t [ vueify ] -t [ envify --NODE_ENV=development ] -e src/app.js -o dist/index.js",
    "build:prod": "browserify -t [ babelify ] -t [ vueify ] -t [ envify --NODE_ENV=production ] -e src/app.js -o dist/index.js",
    "files": "mkdir -p dist/assets && cp src/index.html dist/index.html && cp src/favicon.ico dist/ && cp src/assets/* dist/assets/"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^5.0.1",
    "dotenv-webpack": "^1.5.5",
