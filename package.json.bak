    "fastclick": "^1.0.6",
    "jquery": "^3.1.1",
    "path": "^0.12.7",
    "qp-element-ui": "^1.0.0-rc.8.1",
    "vue": "^2.2.2",
    "vue-resource": "^1.3.4",
    "vue-router": "^2.3.0",
    "webpack": "^4.1.1"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.26.0",
    "babel-plugin-component": "0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-vue": "^0.1.0",
    "babelify": "^8.0.0",
    "browserify": "^14.1.0",
